<form action="/planeRoute/createPlane" method="POST">
    <fieldset>
        <legend>Create Mission</legend>
        <div class="flex-container">
            <input class="flex-item" type="number" id="tailNum" name="tailNum" placeholder="Tail Number"> <br/>
            <input class="flex-item" type="text" id="model" name="model" placeholder="Aircraft Model"> <br/>
        </div>
    </fieldset>
    <br />
    <input class="btn" type="submit" name="submit" value="Submit Form">
    <a class="btn" href="/userRoute/dashboard">CANCEL</a>
</form>